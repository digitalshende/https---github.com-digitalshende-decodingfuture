	/* Main container to hold the boxes */
    .container {
      display: flex;
      justify-content: center;
      align-items: center;
      padding: 50px;
      gap: 30px; /* Adds space between the two boxes */
    }

    /* Style for each box */
    .box {
      background-color: #154360;
      color: white;
      padding: 40px;
      width: 45%; /* Each box takes 45% of the container's width */
      border-radius: 8px;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
      opacity: 0;
      transform: translateY(50px);
      transition: opacity 1s ease-in-out, transform 1s ease-in-out;
      animation: fadeInUp 1s forwards;
    }

    /* Keyframes for the fade-in-up animation */
    @keyframes fadeInUp {
      0% {
        opacity: 0;
        transform: translateY(50px);
      }
      100% {
        opacity: 1;
        transform: translateY(0);
      }
    }

    /* Heading style with floating effect */
    .box h2 {
      font-size: 36px;
      margin-bottom: 20px;
      font-weight: bold;
      animation: float 2s ease-in-out infinite alternate;
    }

    /* Floating effect for headings */
    @keyframes float {
      0% {
        transform: translateY(0);
      }
      100% {
        transform: translateY(-10px);
      }
    }

    /* Paragraph style */
    .box p {
      font-size: 20px;
      line-height: 1.8;
      margin: 0;
      padding: 0;
      font-weight: lighter;
      opacity: 0;
      animation: fadeInText 1.5s ease-in-out 1s forwards;
    }

    /* Text fade-in animation */
    @keyframes fadeInText {
      0% {
        opacity: 0;
      }
      100% {
        opacity: 1;
      }
    }

    /* Hover effect for the mission statement text */
